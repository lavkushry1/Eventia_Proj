# Frontend Environment Variables Example
# Rename this file to .env and modify values as needed

# API and Static URLs (required)
VITE_API_BASE_URL=http://localhost:3000/api/v1
VITE_STATIC_URL=http://localhost:3000/static

# Payment Configuration
VITE_PAYMENT_VPA=eventia@axis
VITE_ENABLE_QR=true
VITE_DOMAIN=localhost
VITE_MERCHANT_NAME=Eventia Ticketing
VITE_PAYMENT_ENABLED=true
VITE_QR_IMAGE_URL=/static/uploads/qr_code.png

# Debug mode
VITE_DEBUG=true
